{
  "regions": ["fra1"],
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 60
    },
    "app/api/generations/create/route.ts": {
      "maxDuration": 60
    },
    "app/api/webhook/replicate/route.ts": {
      "maxDuration": 60
    },
    "app/api/webhook/fal/route.ts": {
      "maxDuration": 60
    },
    "app/api/generations/cleanup-stale/route.ts": {
      "maxDuration": 120
    },
    "app/api/cron/cleanup/route.ts": {
      "maxDuration": 120
    },
    "app/api/keyframes/generate/route.ts": {
      "maxDuration": 300
    },
    "app/api/keyframes/status/[id]/route.ts": {
      "maxDuration": 60
    },
    "app/api/health/route.ts": {
      "maxDuration": 5
    },
    "app/api/generations/list/route.ts": {
      "maxDuration": 30
    },
    "app/api/generations/sync-status/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 3 * * *"
    }
  ]
}












